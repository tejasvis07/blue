Exp 1 LED BLINKING AND GPIO 
CONTROL 
CONNECTION: 
Pin 13 (digital) = LED anode(Long leg) 
GND              = LED cathode (-) 
CODE 
const int LED1 = 8; 
const int LED2 = 9; 
const int LED3 = 4; 
void setup() { 
  pinMode(LED1, OUTPUT); 
  pinMode(LED2, OUTPUT); 
  pinMode(LED3,OUTPUT); 
} 
void loop() { 
  digitalWrite(LED1, HIGH); 
  digitalWrite(LED2, LOW); 
  digitalWrite(LED3, LOW); 
  delay(500); 
  digitalWrite(LED1, LOW); 
  digitalWrite(LED2, HIGH); 
  digitalWrite(LED3, LOW); 
  delay(500);  
  digitalWrite(LED1, LOW); 
  digitalWrite(LED2, LOW); 
  digitalWrite(LED3, HIGH); 
  delay(500);    
} 
